// Create at least 10 tests. Have at least 5 tests evaluate to True and another 5 tests evaluate to False.

//1/5 tests giving true
let car = 'subaru';
console.log("Is car == 'subaru'? I predict True.");
console.log(car == 'subaru');

//2/5 tests giving true
let colour = 'pink';
console.log("Is colour == 'pink'? I predict True.");
console.log(colour == 'pink');

//3/5 tests giving true
let drink = 'tea';
console.log("Is drink == 'tea'? I predict True.");
console.log(drink == 'tea');

// //4/5 tests giving true
