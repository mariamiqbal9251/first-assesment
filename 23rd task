// //5/5 tests giving true
let food = 'pizza';
console.log("Is food == 'pizza'? I predict True.");
console.log(food== 'pizza');

//1/5 tests giving false
let car2 = 'subaru';
console.log("Is car == 'subaru'? I predict True.");
console.log(car2 == 'suban');

//2/5 tests giving false
